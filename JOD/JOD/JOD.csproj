<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>joey</Authors>
    <Description>OpenDocument操作库</Description>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resource\mimetype" />
    <None Remove="Resource\thumbnail.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resource\mimetype" />
    <EmbeddedResource Include="Resource\thumbnail.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="4.5.1" />
    <PackageReference Include="System.IO.Compression.ZipFile" Version="4.3.0" />
  </ItemGroup>

</Project>
